{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": ".",
    "allowJs": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@payload/*": ["src/lib/payload/*"],
      "@api/*": ["src/lib/api/*"],
      "@lib/*": ["src/lib/*"],
      "@common/*": ["src/common/*"],
      "@components/*": ["src/components/*"],
      "@hook/*": ["src/hook/*"],
      "@pages/*": ["src/pages/*"],
      "@assets/*": ["src/assets/*"],
      "@constrant/*": ["src/constrant/*"],
      "@/*": ["src/*"]
    }
  },
  "include": ["."]
}
